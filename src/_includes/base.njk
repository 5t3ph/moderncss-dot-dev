{%- set layoutName %}{{ layout | stripFilename }}{% endset -%}
{%- set pageTitle %}{% if title %}{{ title }} | {% endif %}{{ site.siteName }}{% endset -%}
{%- set pageDescription %}{% if description %}{{ description }}{% else %}{{ site.siteDescription }}{% endif %}{% endset -%}
{%- set pageSocialImg %}{{ site.url }}/img/{% if title %}{{ title | slug }}{% else %}home{% endif %}.png{% endset -%}
